				{
					"name": "gui_storage::activate_coins",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"// Strip JSON Comments",
									"const rawData = pm.request.body.toString();",
									"const strippedData = rawData.replace(/\\\\\"|\"(?:\\\\\"|[^\"])*\"|(\\/\\/.*|\\/\\*[\\s\\S]*?\\*\\/)/g, (m, g) => g ? \"\" : m)",
									"",
									"pm.request.body.update(strippedData);"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"userpass\": \"{{userpass}}\",\n    \"method\": \"gui_storage::activate_coins\",\n    \"mmrpc\": \"2.0\",\n    \"params\": {\"account_id\": \"{{account_id}}\", \"coins\": [\"KMD\", \"LTC\"]}\n}"
						},
						"url": {
							"raw": "{{address}}",
							"host": [
								"{{address}}"
							]
						}
					},
					"response": []
				},
